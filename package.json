{
  "name": "everydate",
  "version": "1.1.1",
  "description": "A simple date recurrence tool",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run test && npm run clean && webpack",
    "test": "jest",
    "test:watch": "jest --watch",
    "flow": "flow",
    "prettier": "prettier \"*/**/*.js\" --ignore-path ./.prettierignore --write",
    "prepublishOnly": "npm run build"
  },
  "author": "Henrikas Kuzmickas <henry.kuzmick@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrykuzmick/everydate.git"
  },
  "keywords": [
    "everydate",
    "date",
    "dates",
    "recur",
    "recurrence",
    "date-fns",
    "moment",
    "everyday",
    "match",
    "schedule",
    "calendar"
  ],
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.29.0",
    "babel-preset-env": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "eslint": "^4.16.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.63.1",
    "jest": "^22.1.4",
    "prettier": "^1.10.2",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.10.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./dist"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ]
  }
}
